{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-trailing-spaces": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "camelcase": "off",
        "newline-per-chained-call": "off"
    }
}
